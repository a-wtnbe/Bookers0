<h1>新規投稿</h1>
<!-- form_with部分 -->
<%= form_with model: @list, url:'/list', local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>